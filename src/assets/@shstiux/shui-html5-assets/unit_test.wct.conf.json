{
  "plugins": {
    "istanbub": {
      "dir": "./coverage",
      "reporters": ["text-summary", "lcov", "clover"],
      "include": [
        "./src/*.js"
      ],
      "exclude": [
        "/polymer/polymer.js",
        "/platform/platform.js",
        "./src/sh-menu-group.js"
      ],
      "moduleResolution": "node"
    },
    "local": {
        "browsers": ["chrome"]
    }
  },
  "suites":["unit_test/"]
}